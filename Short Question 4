#include <iostream>
#include <vector>
using namespace std;

void reverseVector(vector<int> &v) {
    int start = 0;
    int end = v.size() - 1;

    while (start < end) {
        int temp = v[start];
        v[start] = v[end];
        v[end] = temp;

        start++;
        end--;
    }
}

int main() {
    vector<int> v = {1, 2, 3, 4, 5};

    reverseVector(v);

    cout << "Reversed vector: ";
    for (int x : v) {
        cout << x << " ";
    }

    return 0;
}
