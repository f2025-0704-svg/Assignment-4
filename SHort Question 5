#include <iostream>
#include <vector>
using namespace std;

int main() {
    int N, target;
    cout << "Enter number of elements: ";
    cin >> N;

    vector<int> v(N);
    for (int i = 0; i < N; i++) {
        cin >> v[i];
    }

    cout << "Enter number to count: ";
    cin >> target;

    int count = 0;
    for (int i = 0; i < N; i++) {
        if (v[i] == target)
            count++;
    }

    cout << "Occurrences: " << count << endl;

    return 0;
}
