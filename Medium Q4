#include <iostream>
#include <vector>
using namespace std;

int main() {
    vector<int> nums = {1, 5, 7, -1, 5};
    int target = 6;
    int count = 0;

    for (int i = 0; i < nums.size(); i++) {
        for (int j = i + 1; j < nums.size(); j++) {
            if (nums[i] + nums[j] == target) {
                count++;
            }
        }
    }

    cout << "Number of pairs with sum " << target << " is: " << count;

    return 0;
}
