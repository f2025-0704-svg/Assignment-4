#include <iostream>
#include <vector>
using namespace std;

int main() {
    vector<int> nums = {1, 2, 3, 4, 5, 6};

    vector<int> result;

    for (int i = 0; i < nums.size(); i++) {
        if (nums[i] % 2 != 0) {   // keep odd numbers
            result.push_back(nums[i]);
        }
    }

    cout << "Vector after removing even numbers: ";
    for (int i = 0; i < result.size(); i++) {
        cout << result[i] << " ";
    }

    return 0;
}
